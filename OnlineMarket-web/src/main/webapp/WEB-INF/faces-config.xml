<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
              xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">

    <lifecycle>
        <phase-listener>
            com.onlinemarket.util.AuthenticationPhaseListener
        </phase-listener>
    </lifecycle>
   <navigation-rule>
        <navigation-case>
            <from-outcome>login</from-outcome>
            <to-view-id>/users/login.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>create</from-outcome>
            <to-view-id>/users/create.xhtml</to-view-id>            
        </navigation-case>
        <navigation-case>
            <from-outcome>app-main</from-outcome>
            <to-view-id>/annonces/index.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>createStep2</from-outcome>
            <to-view-id>/annonces/createStep2.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>createStep3</from-outcome>
            <to-view-id>/annonces/createStep3.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>detail</from-outcome>
            <to-view-id>/annonces/detail.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>detail2</from-outcome>
            <to-view-id>/administration/detail.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>annonce_creee</from-outcome>
            <to-view-id>/annonces/annonce_succeeded.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>validee</from-outcome>
            <to-view-id>/administration/index.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>commande_succes</from-outcome>
            <to-view-id>/commandes/index.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>detail3</from-outcome>
            <to-view-id>/commandes/detail.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        
    </navigation-rule>
   

</faces-config>

